{
  "name": "vue-solar-lunar",
  "version": "1.0.0",
  "description": "支持Vue | Vue3的公历转农历插件",
  "main": "dist/main.js",
  "types": "dist/types/main.d.ts",
  "private": false,
  "publisher": "magicalprogrammer@qq.com",
  "scripts": {
    "build": "tsc",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
